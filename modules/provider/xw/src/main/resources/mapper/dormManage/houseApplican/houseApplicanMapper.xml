    <select id="findPassPending" parameterType="HouseApplican" resultType="HouseApplican">
        SELECT c.* from (
        SELECT
          a.*, b.status as teacherStatus
        FROM
            xw_house_applican a,
            xw_house_applican_teachers b
        <where>
            a.id = b.house_applican_id
            and a.school_id =b.school_id
