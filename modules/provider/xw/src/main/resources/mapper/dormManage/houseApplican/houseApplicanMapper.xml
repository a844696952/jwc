    <insert id="saveHouseApplican" parameterType="HouseApplican">
        insert into xw_house_applican
         (
            id,
            school_id,
            create_time,
            applican_type,
            start_time,
            end_time,
            remark,
            initiate_id,
            initiate_name,
            initiate_tel,
            initiate_port,
            status,
            progress,
            head_teacher_id,
            stu_id
        ) values
         (
                #{id}
                ,
                #{schoolId}
                ,
                now()
                ,
                #{applicanType}
                ,
                #{startTime}
                ,
                #{endTime}
                ,
                #{remark}
                ,
                #{initiateId}
                ,
                #{initiateName}
                ,
                #{initiateTel}
                ,
                #{initiatePort}
                ,
                #{status}
                ,
                #{progress}
                ,
                #{headTeacherId}
                ,
                #{stuId}

         )
    </insert>
